
{
  "namespace": "dock_bar",
  
  "dock_background": {
    "type": "image",
    "texture": "textures/ui/dialog_background_opaque"
  },
  
  "play_button@icon_button.light_glyph_button": {
    "$pressed_button_name": "button.menu_play",
    "$button_name": "menu.play",
    "size": [ 25, 25 ],
	"$glyph_texture": "textures/ui/buttons/icons/play_icon"
  },
  
  "feed_button@icon_button.light_glyph_button": {
    "$pressed_button_name": "button.menu_feed",
    "size": [ 25, 25 ],
	"$glyph_texture": "textures/ui/buttons/icons/feed_icon"
  },
  
  "qicon_buttonck_play_button@icon_button.general_icon_button": {
    "$pressed_button_name": "button.menu_qicon_buttonck_play",
    "size": [ 25, 25 ],
	"$glyph_texture": "textures/ui/buttons/icons/play_icon"
  },
  
  "achievements_button@icon_button.light_glyph_button": {
    "$pressed_button_name": "button.menu_achievements",
    "$button_name": "menu.achievements",
    "size": [ 25, 25 ],
	"$glyph_texture": "textures/ui/buttons/icons/achievements_icon"
  },
  
  "settings_button@icon_button.light_glyph_button": {
    "$pressed_button_name": "button.menu_settings",
