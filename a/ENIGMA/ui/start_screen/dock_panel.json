    "size": [ 25, 25 ],
	"$glyph_texture": "textures/ui/buttons/icons/settings_icon"
  },
  
  "store_button@icon_button.light_glyph_button": {
    "$pressed_button_name": "button.menu_store",
    "$button_name": "menu.store",
    "size": [ 25, 25 ],
