	 "cancel_button@common.button": {
        "size": [ 16, "16px" ],
        
        "$pressed_button_name|default": "button.menu_leave_screen",
        
        "$default_button_texture|default": "",
        "$hover_button_texture|default": "",
        "$pressed_button_texture|default": "",
        "$locked_button_texture|default": "",
        
        "$default_state|default": false,
        "$hover_state|default": false,
        "$pressed_state|default": false,
        
        "controls": [
            {
                "default@ui.cancel_button_panel": {
                    "layer": 1,
                    "$border_color": "$light_border_default_color",
                    "$border_layer": 2,
                    "$default_state": true
                }
            },
            {
                "hover@ui.cancel_button_panel": {
                    "layer": 2,
                    "$border_color": "$light_border_hover_color",
                    "$border_layer": 3,
                    "$hover_state": true
                }
            },
            {
                "pressed@ui.cancel_button_panel": {
                    "layer": 3,
                    "$border_color": "$light_border_pressed_color",
                    "$border_layer": 3,
                    "$pressed_state": true
                }
            }
        ]
    },
	
	"cancel_button_panel": {
        "type": "stack_panel",
        "orientation": "horizontal",
        "$hide_button_tips|default": false,
        "size": [ "100%c", "16px" ],
        "controls": [
            {
                "button":{
                    "type": "panel",
                    "size": [ 16 , 16 ],
                    "controls": [
                        {
                            "button": {
                                "type": "image",
                                "size": [ 8, 8 ],
                                "texture": "textures/ui/ui_cancel_button"
                            }
                        },
                        {
                            "border": {
                                "type": "image",
                                "size": [ 8, 8 ],
                                "texture": "textures/ui/ui_exit_focus_border",
                                "color": "$border_color",
                                "layer": "$border_layer"
                            }
                        }
                    ]
                }
            },
            {
                "chevron_panel": {
                    "type": "panel",
                    "ignored": "$hide_button_tips",
                    "size": [ 8 , 16 ],
                    "controls": [
                        {
                            "left_chevron": {
                                "type": "image",
                                "size": [ 4, 7 ],
                                "offset": "@ui.exit_tips_animation_offset_1",
                                "alpha": "@ui.exit_tips_animation_alpha_1",
                                "animation_reset_name": "screen_animation_reset",
                                "propagate_alpha": true,
                                "disable_anim_fast_forward": true,
                                "texture": "textures/ui/chevron_left",
                                "color": "$title_color"
                            }
                        }
                    ]
                }
            },
            {
                "label_panel": {
                    "type": "panel",
                    "ignored": "$hide_button_tips",
                    "size": [ "100%c", 16 ],
                    "controls": [
                        {
                            "label": {
                                "type": "label",
                                "offset": "@ui.exit_tips_animation_offset_1",
                                "alpha": "@ui.exit_tips_animation_alpha_1",
                                "animation_reset_name": "screen_animation_reset",
                                "propagate_alpha": true,
                                "disable_anim_fast_forward": true,
                                "color": "$title_color",
                                "text": "ui_cancel_button.tips"
                            }
                        }
                    ]
                }
            }
        ]
    },
	
	"exit_tips_animation_offset_1": {
        "anim_type": "offset",
        "easing": "out_cubic",
        "duration": 0.5,
        "from": [ 0, 0 ],
        "to": [ -4, 0 ],
        "next": "@ui.exit_tips_animation_offset_2"
    },
    
    "exit_tips_animation_offset_2": {
        "anim_type": "offset",
        "easing": "out_cubic",
        "duration": 0.5,
        "from": [ -4, 0 ],
        "to": [ 0, 0 ],
        "next": "@ui.exit_tips_animation_offset_3"
    },
    
    "exit_tips_animation_offset_3": {
        "anim_type": "offset",
        "easing": "out_cubic",
        "duration": 0.5,
        "from": [ 0, 0 ],
        "to": [ 0, 0 ],
        "next": "@ui.exit_tips_animation_offset_1"
    },
    
    "exit_tips_animation_alpha_1": {
        "anim_type": "alpha",
        "duration": 4.25,
        "from": 0,
        "to": 0,
        "next": "@ui.exit_tips_animation_alpha_2"
    },
    
    "exit_tips_animation_alpha_2": {
        "anim_type": "alpha",
        "duration": 0.25,
        "from": 0,
        "to": 1,
        "next": "@ui.exit_tips_animation_alpha_3"
    },
    
    "exit_tips_animation_alpha_3": {
        "anim_type": "alpha",
        "duration": 2.75,
        "from": 1,
        "to": 1,
        "next": "@ui.exit_tips_animation_alpha_4"
    },
    
    "exit_tips_animation_alpha_4": {
        "anim_type": "alpha",
        "duration": 0.25,
        "from": 1,
        "to": 0,
        "next": "@ui.exit_tips_animation_alpha_1"
    },
	
