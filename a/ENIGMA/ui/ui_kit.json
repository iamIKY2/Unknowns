                "requires": "$include_menu_screen",
                "$screen_type": "ui.include_menu_screen",
                "$desktop_screen_size": [ 370, "100% - 50px" ]
            },
            {
                "requires": "$include_menu_store_screen",
                "$screen_type": "ui.include_menu_store_screen",
                "$desktop_screen_size": [ "100%", "100%" ]
            },
            {
                "requires": "$include_title_store_screen",
                "$screen_type": "ui.include_title_screen",
                "$desktop_screen_size": [ "100%", "100%" ]
            }
        ],
        */
        "$screen_type": "ui.include_menu_screen",
        "$bk": "",
        "$desktop_screen_size": [ 370, "100% - 50px" ],
        "controls": [/*
            {
                "screen_background": {
                    "type": "image",
                    "texture":"background/2",
                    "uv_size":[1920,1080],
                    "uv": "@ui.bg_10000",
                    "layer": -2
                }
            },*/
            {
                "screen@$screen_type": {
                    "layer": 1,
                    "size": "$screen_size",
                    "variables": [
                        {
                            "requires": "$desktop_screen",
                            "$screen_size": "$desktop_screen_size"
                        },
                        {
                            "requires": "($pocket_screen or $is_holographic)",
                            "$screen_size": [ "100%", "100%" ]
                        }
                    ]
                }
            },
            {
                "bk@$bk":{
                    "size":["150%","150%"]
                }
            },
            {
                "screen_shadow": {
                    "type": "panel",
                    "layer": 1,
                    "size": "$screen_size",
                    "variables": [
                        {
                            "requires": "$desktop_screen",
                            "$screen_size": "$desktop_screen_size"
                        },
                        {
                            "requires": "($pocket_screen or $is_holographic)",
                            "$screen_size": [ "100%", "100%" ]
                        }
                    ],
                    "controls": [
                        {
                            "shadow_TEXTURE": {
                                "type": "image",
                                "layer": 2,
                                "nineslice_size": 20,
                                "base_size": [ 100, 100 ],
                                "texture": "textures/ui/window_shadow",
                                "size": [ "100% + 22px", "100% + 22px" ]
                            }
                        }
                    ]
                }
            }
        ]
    },
    
