{
  "namespace": "desktop_start",
  "widgets_panel": {
    "type": "panel",
    "controls": [
      {
        "dock_bar@ui.dock_bar_panel": {
          "size": [
            "100%",
            "38px"
          ]
        }
      },
      {
        "text_panel@start.text_panel": {
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle",
          "ignored": "(not $hide_widgets)"
        }
      }
    ]
  },
  "home_panel": {
    "type": "panel",
    "layer": 1,
    "controls": [
      {
        "dock_bar@ui.dock_bar_panel": {
          "size": [
            "100%",
            "38px"
          ]
        }
      },
      {
        "text_panel@start.text_panel": {
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle",
          "ignored": "(not $hide_widgets)"
        }
      }
    ]
  },
  "start_screen_content": {
    "type": "panel",
    "$achievements_ignored|default": false,
    "$feedback_ignored|default": false,
    "$store_ignored|default": false,
    "controls": [
      {
        "control_center_panel@desktop_start.home_panel": {
          "layer": 2
        }
      }
    ]
  }
}